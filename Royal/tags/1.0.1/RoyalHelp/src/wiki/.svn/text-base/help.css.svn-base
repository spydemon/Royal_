h1 {text-indent:         10px;
   font-family:          Verdana;
   font-size:            12pt;
   color:                navy;
   }

h2 {text-indent:         0px;
   font-family:          Verdana;
   font-size:            12pt;
   color:                black;
   }
   
h3 {text-indent:         5px;
   font-family:          Verdana;
   font-size:            12pt;
   color:                black;
   }

p {text-indent:          10px;
   font-family:          Verdana;
   font-size:            12pt;
   color:                black;
   }
   
table {margin-left:      40px;
    margin-right:        40px;
    width:               100%;
    border-spacing:      -1px;
   }
   
th {background-color:    #aebfe2;
   }
   
td {border-style:        solid; 
    border-width:        1px; 
    border-color:        #aebfe2;
    background-color:    #ffffff;
    border-collapse:     collapse;
   }

em {color:                navy;
   }