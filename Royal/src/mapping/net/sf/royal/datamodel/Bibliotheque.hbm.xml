<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="net.sf.royal.datamodel.Bibliotheque">
		<id name="id">
			<generator class="native" />
		</id>

		<property name="name" column="name_biblio" />
		<property name="address" column="address_biblio" />
		<property name="phone" column="phone_biblio" />
	</class>
</hibernate-mapping>
